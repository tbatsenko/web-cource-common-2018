<header class="bureau__container bureau__header">
    {{#header}}
        <a class="header__logo">{{logo}}</a>
        <nav class="header__menu">
            {{#menu}}
                <a class="menu__item" href="#">{{.}}</a>
            {{/menu}}
        </nav>
        <button class="header__user-button">{{user}}</button>
    {{/header}}
</header>

<main class="bureau__container bureau__posts">
    <article class="posts__section posts__advert">
        {{#posts.[0]}}
            <a class="advert__image" href="#">
                <img class="advert__image_content" src="/images/kids.png" alt="Shop-fans">
            </a>
            <a class="advert__description" href="#">
                <u class="advert__description_underlined">{{description.[0]}}</u> {{description.[1]}}
            </a>
        {{/posts.[0]}}
    </article>

    <div class="posts__section posts__courses">
        {{#posts.[1]}}
            <article class="courses__post courses__latest">
                <a href="#" class="courses__latest-container">
                    {{#latest}}
                        <div class="latest__countdown">
                            <div class="countdown__number">{{countdown-number}}</div>
                            {{#secondary-text}}
                                <p class="countdown__secondary-text">{{.}}</p>
                            {{/secondary-text}}
                        </div>
                        <div class="latest__announcement">
                            <h4 class="announcement__title">{{announcement-title}}</h4>
                            <p class="announcement__link">{{link.[0]}}<u
                                    class="announcement__link_underlined">{{link.[1]}}</u></p>
                        </div>
                    {{/latest}}
                </a>
            </article>
            <article class="courses__post courses__other">
                {{#other}}
                    <h4 class="other__title">{{title}}</h4>
                    <ul class="other__course-list">
                        {{#courses}}
                            <li class="course-list__item">
                                <a href="#" class="item__content">{{.}}</a>
                            </li>
                        {{/courses}}
                    </ul>
                {{/other}}
            </article>
        {{/posts.[1]}}
    </div>

    <div class="posts__section posts__educational">
        {{#posts.[2]}}
            <article class="educational__post">
                <a class="educational__content" href="#">
                    <h4 class="educational__title">{{titles.[0]}}</h4>
                    <img class="day-icon__image" src="/images/icon_of_day.jpg">
                    <p class="day-icon__description">{{descriptions.[0]}}</p>
                    <address class="educational__author educational__author_no-separate">{{authors.[0]}}</address>
                </a>
            </article>
            <article class="educational__post">
                <a class="educational__content" href="#">
                    <h4 class="educational__title">{{titles.[1]}}</h4>
                    <p class="article__description">{{descriptions.[1]}}</p>
                    <address class="educational__author">{{authors.[1]}}</address>
                </a>
            </article>
            <article class="educational__post educational__post_important">
                <a class="educational__content" href="#">
                    <h4 class="educational__title educational__title_important">{{titles.[2]}}</h4>
                    <p class="article__description educational__description_important">{{descriptions.[2]}}</p>
                    <address class="educational__author">{{authors.[2]}}</address>
                </a>
            </article>
        {{/posts.[2]}}
    </div>

    <article class="posts__section posts__hottest-books">
        <a class="hottest-books__book hottest-books__book_advices">
            <img src="/images/advice.png" class="hottest-books__book-image" alt="Советы">
        </a>
        <a class="hottest-books__book hottest-books__book_manifest">
            <img src="/images/manifest.jpg" class="hottest-books__book-image"
                 alt="Манифест издательства Бюро горбунова">
        </a>
    </article>

    <article class="posts__section posts__materials">
        {{#posts.[3]}}
            <a class="materials__fonts" href="#">
                <svg class="fonts__background" width="100%" height="100%" viewBox="0 0 197.434 388.365">
                    <path class="a"
                          d="M98.8,99.8C55.3,56.4,11.8,10.5,2.5.7L0.7,2.2V386l2.1,1.7c9.7-10.3,52.4-55.3,95.3-98.1,49.2-49.1,98.8-95.3,98.8-95.3s-49-45.6-98.1-94.5"
                          transform="translate(-0.2 -0.016)">
                    </path>
                </svg>
                <a class="fonts__link" href="#">
                    <div class="fonts__icon">
                        <svg viewBox="0 0 64 64" class="icon__svg">
                            <g>
                                <path class="icon__svg-letter"
                                      d="M0,14.2h25.6v7H8.3v8.3h8.4c8.5,0,12.3,4.4,12.3,11.2C29,48,23.3,53,15,53H0V14.2z M8.3,36.3v9.7h5.9   c4.5,0,6.6-1.5,6.6-5.4c0-3.1-1.9-4.4-5.6-4.4H8.3z"></path>
                                <path class="icon__svg-letter"
                                      d="M48.8,53.6c-10.2,0-13.8-6-13.8-17.4c0-11.5,4.3-18.9,16.6-23.2l7.6-2.7l2.6,6.4c-3.5,1.4-8.1,2.9-12.2,4.6   c-3.1,1.3-4.7,3-5.5,4.2l-1.3,1.7l0.4,0.2c1.6-1.7,4.6-3.4,8.3-3.4C60.2,24.1,64,29,64,37C64,47,59.3,53.6,48.8,53.6z M49.1,47.1   c4.7,0,7-3.4,7-9.7c0-5.2-2.2-6.9-6.3-6.9c-2.7,0-5.3,1.1-6.8,2.8C43,34.5,43,35.6,43,36.7C43,43.8,44.8,47.1,49.1,47.1z"></path>
                            </g>
                        </svg>
                    </div>
                    <h4 class="fonts__title">{{title}}</h4>
                    <p class="fonts__amount">{{fonts-amount}}</p>
                </a>
            </a>
            <a class="materials__image" href="#">
                <img class="materials__image_content" src="/images/burosign.jpg" alt="Бюросайн">
            </a>
            <a class="materials__description" href="#">
                <u class="advert__description_underlined">{{description.[0]}}</u>
                {{description.[1]}}
            </a>
        {{/posts.[3]}}
    </article>
    <div class="posts__section posts__books">
        {{#posts.[4]}}
            <h4 class="books__title">{{title}}</h4>
            <ul class="books__list">
                {{#hottest.[0]}}
                    <li class="books__book books__book_hottest">
                        <a href="#" class="book__content">
                            <img class="book__image" src="/images/bookshelf.png" alt="">
                            <h4 class="book__title">{{books-title}}</h4>
                            <p class="book__topic">{{book-topic}}</p>
                            <p class="book__price">{{price}}</p>
                        </a>
                    </li>
                {{/hottest.[0]}}
                {{#books}}
                    <li class="books__book">
                        <a href="#" class="book__content">
                            <img class="book__image" src="{{books-illustration}}" alt="">
                            <h4 class="book__title">{{books-title}}</h4>
                            <p class="book__topic">{{book-topic}}</p>
                            <p class="book__price">{{price}}</p>
                        </a>
                    </li>
                {{/books}}
                {{#pre-order}}
                    <li class="books__book">
                        <a href="#" class="book__content">
                            <img class="book__image" src="{{books-illustration}}" alt="">
                            <h4 class="book__title">{{books-title}}</h4>
                            <p class="book__topic">{{book-topic}}</p>
                            <p class="book__price book__price_pre-order">{{price}}</p>
                        </a>
                    </li>
                {{/pre-order}}
                {{#hottest.[1]}}
                    <li class="books__book books__book_hottest">
                        <a href="#" class="book__content book__content_centered">
                            <img class="book__image" src="/images/present-book.jpg" alt="">
                            <h4 class="book__title">{{books-title}}</h4>
                            <p class="book__topic">{{book-topic}}</p>
                            <p class="book__price book__price_sell">{{price}}</p>
                        </a>
                    </li>
                {{/hottest.[1]}}
            </ul>
        {{/posts.[4]}}
    </div>

    <div class="posts__section posts__courses posts__courses_separate">
        {{#posts.[5]}}
            <article class="courses__soon">
                <a href="#" class="soon__content">
                    <h4 class="soon__title">{{titles.[0]}}</h4>
                    <button class="soon__sign-up soon__sign-up_blue">{{sign-in}}</button>
                    <div class="soon__info">
                        <time class="soon__course-time" datetime="27-11-2018">{{dates.[0]}}</time>
                        <address class="soon__author">{{authors.[0]}}</address>
                    </div>
                </a>
            </article>
            <article class="courses__soon">
                <a href="#" class="soon__content">
                    <h4 class="soon__title">{{titles.[1]}}</h4>
                    <button class="soon__sign-up soon__sign-up_black">{{sign-in}}</button>
                    <div class="soon__info">
                        <time class="soon__course-time" datetime="27-11-2018">{{dates.[1]}}</time>
                        <address class="soon__author">{{authors.[1]}}</address>
                    </div>
                </a>
            </article>
        {{/posts.[5]}}
    </div>

    <a class="posts__section posts__full-page-advert">
        <img class="full-page-advert__image" src="/images/book-collection.png">
    </a>

    <article class="posts__section posts__hottest-books">
        <a class="hottest-books__book hottest-books__book_tomato" href="#">
            <img src="/images/tomato_book.png" class="hottest-books__book-image" alt="Tvigle">
        </a>
        <a class="hottest-books__book hottest-books__book_chess" href="#">
            <img src="/images/chess_book.png" class="hottest-books__book-image"
                 alt="Путишествие в шахматное королевство">
        </a>
    </article>

    <div class="posts__section posts__cafe">
        {{#posts.[6]}}
            <a class="cafe__image" href="#">
                <img class="cafe__image_content"
                     src="https://bureau.ru/coworkafe/i/webcam_frame.jpg?v=0.7156392229205644" alt="Коворка">
            </a>
            <a class="cafe__about" href="#">
                <h4 class="cafe__logo">{{cafe-name}}</h4>
                <div class="cafe__schedule">
                    <time class="cafe__time">
                        {{time.[0]}}
                        <br>
                        {{time.[1]}}
                    </time>
                    <address class="cafe__link">{{link}}</address>
                </div>
                <div class="cafe__product">
                    {{product.[0]}}<br>{{product.[1]}}
                    <p class="product__price">{{price}}</p>
                    <div class="product__illustration">{{illustration}}</div>
                </div>
            </a>
            <div class="cafe__event">
                {{#event}}
                    <h4 class="event__title">{{title}}</h4>
                    <p class="event__topic">{{topic}}</p>
                    <div class="event__info">
                        <time class="event__time">{{time}}</time>
                        <p class="event__author">{{author}}</p>
                        <p class="event__places">{{places}}</p>
                    </div>
                {{/event}}
            </div>
        {{/posts.[6]}}
    </div>

    <article class="posts__section posts__statistics">
        <a class="statistics__client" href="#">
            <img class="statistics__client_image" src="/images/wedding-stat.png" alt="Примерка">
        </a>
        <a class="statistics__social-media" href="#">
            <img class="statistics__social-media_image" src="/images/social-media-stat.png" alt="Соцсети">
        </a>
    </article>

    <article class="posts__section posts__materials posts__materials_reversed">
        {{#posts.[7]}}
            <a class="materials__fonts" href="#">
                <svg class="fonts__background fonts__background_greyed" width="100%" height="100%"
                     viewBox="0 0 197.434 388.365">
                    <path class="a"
                          d="M98.8,99.8C55.3,56.4,11.8,10.5,2.5.7L0.7,2.2V386l2.1,1.7c9.7-10.3,52.4-55.3,95.3-98.1,49.2-49.1,98.8-95.3,98.8-95.3s-49-45.6-98.1-94.5"
                          transform="translate(-0.2 -0.016)">
                    </path>
                </svg>
                <a class="fonts__link fonts__link_books" href="#">
                    <div class="fonts__icon">
                        <svg viewBox="0 0 12.7 9.5" class="icon__svg icon__svg_books">
                            <g>
                                <path class="icon__svg-letter icon__svg-letter_books"
                                      d="M4,8.8c-1.5,0-3.6,0.5-4,0.6V0.8C0,0.8,1.9,0,3.8,0c1,0,2,0.3,2.6,0.5C6.9,0.3,7.9,0,8.9,0c1.9,0,3.8,0.8,3.8,0.8v8.6    c-0.3-0.1-2.5-0.6-4-0.6c-1.1,0-1.9,0.3-2.4,0.6C5.9,9.1,5.1,8.8,4,8.8z M5.9,8.2V1.4C5.4,1.2,4.7,1,3.8,1C2.7,1,1.6,1.3,1,1.6v6.6    C1.8,8,3.1,7.8,3.9,7.8C4.7,7.8,5.3,7.9,5.9,8.2z M11.7,8.2V1.6C11.1,1.3,10,1,8.9,1C8,1,7.3,1.2,6.8,1.4v6.7    c0.6-0.3,1.2-0.4,2-0.4C9.6,7.8,10.9,8,11.7,8.2z"></path>
                            </g>
                        </svg>
                    </div>
                    <h4 class="fonts__title">{{title}}</h4>
                    <p class="fonts__amount">{{works}}</p>
                </a>
            </a>
            <a class="materials__image" href="#">
                <img class="materials__image_content" src="/images/magazine.png" alt="Журнал">
            </a>
            <a class="materials__description" href="#">
                <u class="advert__description_underlined">{{description.[0]}}</u>
                {{description.[1]}}
            </a>
        {{/posts.[7]}}
    </article>

    <article class="posts__section posts__materials">
        {{#posts.[8]}}
            <div class="materials__fonts materials__fonts_bureau-sans">
                <a href="#" class="fonts__general">
                    {{#general}}
                        <h4 class="general__title">{{title}}</h4>
                        <p class="general__example">{{example}}</p>
