<footer class="bureau__container bureau__footer">
    {{#footer}}
        <div class="footer__container footer__contacts">
            {{#contacts}}
                <div class="container__section">
                    {{name.[0]}}
                    <br>
                    {{name.[1]}}
                </div>
                <div class="container__section">
                    <a href="#" class="footer__key-link">{{mail}}</a>
                    <br>
                    <a href="#">{{phone}}</a>
                </div>
            {{/contacts}}
        </div>
        <div class="footer__container footer__address">
            {{#address}}
                <a href="#" class="container__section footer__key-link">{{client}}</a>
                <address class="container__section">
                    {{street.[0]}}
                    <br>
                    {{street.[1]}}
                </address>
            {{/address}}
        </div>
        <div class="footer__container footer__more-options">
            <div class="more-options__content">
                <nav class="more-options__section more-options__social-media">
                    <a class="social-media__link" href="#">
                        <svg viewBox="0 0 30 30" class="link__svg">
