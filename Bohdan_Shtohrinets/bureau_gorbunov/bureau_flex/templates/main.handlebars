                <a class="educational__content" href="#">
                    <h4 class="educational__title">{{titles.[0]}}</h4>
                    <div class="educational__illustration">
                        <img class="illustration__bg" src="/images/nuts.jpg">
                        <div class="illustration__text-inside">{{illustration}}</div>
                    </div>
                    <address class="educational__author">{{authors.[0]}}</address>
                </a>
            </article>
            <article class="educational__post educational__post_reversed">
                <a class="educational__content" href="#">
                    <h4 class="educational__title">{{titles.[1]}}</h4>
                    <p class="article__description">{{descriptions.[0]}}</p>
                    <address class="educational__author">{{authors.[1]}}</address>
                </a>
            </article>
            <article class="educational__post educational__post_important">
                <a class="educational__content" href="#">
                    <h4 class="educational__title educational__title_important">{{titles.[2]}}</h4>
                    <p class="article__description educational__description_important">{{descriptions.[1]}}</p>
                    <address class="educational__author">{{authors.[2]}}</address>
